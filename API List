API
  Account
    GET  /listAccounts
    GET  /accountById
      id
    GET  /accountByEmail
      email
    POST /addRole
      id
      role
    POST /newAccount
      firstName
      lastName
      email
      password
    POST /newLot
      id
      name
      type
      address
      latitude
      longitude
      price
      capacity

  Lot
    GET  /listLots
    GET  /lotById
      id
    GET  /lotByAddress
      address
    POST /entered
      id
    POST /exited
      id
    POST /changeAvailable
      id
    POST /changeReservable
      id
    POST /addMember
      id
      email
      role
    POST /searchNear
      longitude
      latitude

Schema
  Account
    id: id
    firstName: string
    lastName: string
    email: string
    password: string
    phone: string
    photo: object
    parked: boolean
    paid: boolean
    creditCard: string
    lots: map<id, string>
    roles: list<string>

  Lot
    id: id
    name: string
    type: string
    address: string
    location: double[]
    price: string
    capacity: int
    occupied: int
    reservable: boolean
    available: boolean
    members: list<string>
    history: list<point>
